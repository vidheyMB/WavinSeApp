<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="@dimen/small_padding"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="@dimen/small_padding"
        android:weightSum="10">

        <LinearLayout
            android:weightSum="10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_weight="2.1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <ImageView
                    app:srcCompat="@drawable/ic_date"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/activity_vertical_margin"
                    android:layout_height="@dimen/activity_vertical_margin" />

                <TextView
                    android:id="@+id/mem_list_date"
                    android:textColor="@color/dark"
                    android:layout_gravity="center"
                    android:gravity="start|center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:textSize="9sp"
                    android:text="DD/MM/YYYY" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/pointtext"
                android:visibility="visible"
                android:layout_weight="1.9"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <ImageView
                    android:src="@drawable/money"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/activity_vertical_margin"
                    android:layout_height="wrap_content" />

                <TextView
                    android:id="@+id/mem_list_point"
                    android:textColor="@color/dark"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/small_padding"
                    android:textSize="@dimen/h6"
                    android:text="-" />

            </LinearLayout>

            <LinearLayout
                android:layout_weight="2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:src="@drawable/tier"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/activity_vertical_margin"
                    android:layout_height="@dimen/activity_vertical_margin" />

                <TextView
                    android:id="@+id/mem_list_tier"
                    android:textColor="@color/dark"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="2dp"
                    android:textSize="@dimen/h6"
                    android:text="XXXX" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/active_status_host"
                android:layout_weight="1.5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/mem_list_activeImg"
                    app:srcCompat="@drawable/ic_inactive"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/activity_vertical_margin"
                    android:layout_height="@dimen/activity_vertical_margin" />

                <TextView
                    android:id="@+id/mem_list_status"
                    android:textColor="@color/dark"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/small_padding"
                    android:textSize="@dimen/h6"
                    android:text="active" />

            </LinearLayout>

            <LinearLayout
                android:layout_weight="2.5"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/mem_list_verifyImg"
                    app:srcCompat="@drawable/ic_active"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/activity_vertical_margin"
                    android:layout_height="@dimen/activity_vertical_margin" />

                <TextView
                    android:id="@+id/mem_list_approve"
                    android:textColor="@color/dark"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/small_padding"
                    android:textSize="@dimen/h6"
                    android:text="verified" />

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="@dimen/activity_vertical_small_margin"
            android:layout_weight="4"
            android:orientation="horizontal"
            android:background="@color/light_white"
            android:weightSum="10">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_weight="2"
                >

                <ImageView
                    android:id="@+id/general_profile_photo"
                    android:layout_width="@dimen/noraml_btn_height"
                    android:layout_height="@dimen/noraml_btn_height"
                    app:srcCompat="@drawable/ic_person"
                    android:background="@drawable/round_profile_bg"
                    android:padding="@dimen/activity_vertical_small_margin"
                    />
            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_weight="3.5">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:textSize="@dimen/h5"
                    android:text="Name" />

                <TextView
                    android:id="@+id/mem_list_username"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/h5"
                    android:textColor="@color/dark"
                    android:text="XXXXX" />

            </LinearLayout>

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_gravity="center"
                android:layout_weight="2.5">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textColor="@color/black"
                    android:textSize="@dimen/h5"
                    android:text="Membership ID" />

                <TextView
                    android:id="@+id/mem_list_membershipId"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:textSize="@dimen/h5"
                    android:textColor="@color/dark"
                    android:text="YYYYY" />

            </LinearLayout>


            <LinearLayout
                android:id="@+id/call_icon"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:visibility="gone"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_phone_blue"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/interaction"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:gravity="center"
                android:visibility="gone"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_meeting_room"/>

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="4"
            android:orientation="horizontal"
            android:weightSum="4">

            <LinearLayout
                android:layout_weight="1"
                android:background="@color/membershipBlock_color"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/ASMHeader"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="-"
                        android:textColor="@color/colorAccent"
                        android:textSize="@dimen/h6" />
                    <TextView
                        android:id="@+id/mem_list_asm"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="@color/colorAccent"
                        android:text="-" />

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:background="@color/membershipBlock_color"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/SEHeader"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/h6"
                    android:textColor="@color/colorAccent"
                    android:text="-" />

                <TextView
                    android:id="@+id/mem_list_se"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/colorAccent"
                    android:text="-" />
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:background="@color/membershipBlock_color"
                android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/h6"
                    android:textColor="@color/colorAccent"
                    android:text="Created By" />
                <TextView
                    android:id="@+id/mem_list_createdby"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/colorAccent"
                    android:text="-" />

            </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_marginLeft="3dp"
                android:layout_weight="1"
                android:layout_width="match_parent"
                android:background="@color/membershipBlock_color"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textSize="@dimen/h6"
                    android:textColor="@color/colorAccent"
                    android:text="Branch" />
                <TextView
                    android:id="@+id/mem_list_branch"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/colorAccent"
                    android:text="-" />

            </LinearLayout>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


</androidx.cardview.widget.CardView>