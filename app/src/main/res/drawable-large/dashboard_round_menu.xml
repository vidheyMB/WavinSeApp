<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        android:left="10dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/primary_dark_blue"></solid>
            <size android:height="80dp" android:width="80dp"></size>
            <stroke
                android:color="@color/colorAccent"
                android:width="1dp"
                ></stroke>


            <corners
                android:radius="50dp">
            </corners>
        </shape>
    </item>


</layer-list>